@model DataLayer.Entities.Supplementary.FSImage

@{
    ViewData["Title"] = "جزئیات عکس اسلایدر تکه ای";
    Layout = "~/Areas/Manage/Views/Shared/_AdminLayout.cshtml";
}

<h1 class="text-xs-center alert alert-info">جزئیات عکس اسلایدر تکه ای</h1>

<div>

    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.FractionSlider)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.FractionSlider.Id)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Image)
        </dt>
        <dd class="col-sm-10">
            <img src="/images/sliders/fs/@Model.Image" width="75" height="50" />
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Width)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Width)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Height)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Height)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.DataPosition)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DataPosition)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.DataStep)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DataStep)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.ShowRating)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.ShowRating)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.DataIn)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DataIn)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.DataOut)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DataOut)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.DataDelay)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DataDelay)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.DataEaseIn)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DataEaseIn)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.IsActive)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.IsActive)
        </dd>

    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.Id">ویرایش</a> |
    @if (string.IsNullOrEmpty(Model.ReturnUrl))
    {
        <a asp-action="Index">بازگشت</a>
    }
    else
    {
        <a href="@Model.ReturnUrl.Replace("-","/")">بازگشت</a>
    }

</div>
