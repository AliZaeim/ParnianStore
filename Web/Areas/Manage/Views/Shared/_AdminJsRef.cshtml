<script type="text/javascript">

    $(document).ready(function () {
        $(function () {
            // this will get the full URL at the address bar
            var url = window.location.href;
            $(".is-shown a").each(function () {
                // checks if its the same on the address bar
                
                if (url == (this.href)) {
                    //for making parent of submenu active
                    $(this).parent().closest('li').addClass('active');
                    $(this).parent().parent().closest('li').addClass('active open');
                    $(this).parent().parent().parent().parent().closest('li').addClass('active open');
                }
            });
            $(".showspinner").click(function () {
                $("#Progress").fadeIn();
            });
            $("[data-val-required]").each(function () {
                var label = $('label[for="' + $(this).attr("id") + '"]');
                var asterisksHtml = '<span style="color:red;font-size:18px;font-weight:700">&nbsp;*</span>';
                if (label.text().length > 0 && label.html().indexOf(asterisksHtml) === -1) {
                    label.append(asterisksHtml);
                }
            });
        });

    });
</script>